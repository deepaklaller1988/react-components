{
  "name": "Next.js Browser Extension Demo",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Next.js Browser Extension Demo",
    "default_popup": "index.html"
  },
  "permissions":[
    "tabs",
    "scripting"
  ],
  "host_permissions":[
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-eval''unsafe-inline';",
    "sandbox": "sandbox allow-scripting allow-styling"
  }
}
